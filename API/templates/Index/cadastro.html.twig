{% extends 'base.html.twig' %}
{% block Navbar %}
    <h1>Tela de Cadastro</h1>
{% endblock %}
{% block body %}

<form data-form>
    <div class="container">
        <div class="form-group">
              <div class="col col-sm-12 col-md-10">
                    <div class="input-group  mb-3">
                        <label class="col col-md-2" id="l_nome">Nome:</label>
                        <input type="text" class="form-control" placeholder="Digite seu nome" />
                    </div>
              </div>
        </div>

        <div class="form-group">
                <div class="col col-sm-12 col-md-10">
                    <div class="input-group mb-3">
                        <label class="col col-md-2"  id="l_email"> Email: </label>
                        <input type="text" class="form-control" placeholder="Digite seu email" />
                    </div>
                </div>
        </div>

        <div class="form-group">
            <div class="col col-sm-12 col-md-10">
                <div class="input-group mb-3">
                    <label class="col col-md-2"  id="l_numero">Numero: </label>
                    <input type="text" class="form-control" placeholder="Digite seu numero" />
                </div>
            </div>
        </div>
        <div>
            <button type="submit" class="btn btn-primary enviar_form">Enviar</button>
        </div>
    </div>
</form>
{% endblock %}